    "#Prime Number Checker \n",
    "def prime_checker(number) :\n",
    "    for n in  range   ( 2 , number ) :\n",
    "        if number % n == 0 :\n",
    "           print( number ,\"is not a prime number\" )\n",
    "           return\n",
    "    print ( number  , \"is a prime number\" )\n",
    "            \n",
    "prime_checker( int(input()))"
   ]
